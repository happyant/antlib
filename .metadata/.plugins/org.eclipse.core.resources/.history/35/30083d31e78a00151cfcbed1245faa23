#ifndef USER_LIST_H_
#define USER_LIST_H_

#include "handlerlib/handlerlib.h"


#define USERLIST_NUM 39

struct user_account
{
	uint64_t _id;
	uint32_t _time;
	uint32_t _lastused;
	char _state;
	char _name[32];
	char _password[32];
};

struct user_account_group
{
	struct squeue* _caches;
	uint32_t _handlerid;
	uint32_t _last_update_time;
};


//a~z0~9_@.
void userlist_init(int handler_group,int user_cache_max);

void userlist_uninit();

void pushmsg_byname(const char* name,handler_msg* msg);


#endif /* USER_LIST_H_ */
